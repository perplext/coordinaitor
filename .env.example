# API Keys
ANTHROPIC_API_KEY=your_claude_api_key
GOOGLE_GEMINI_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AZURE_API_KEY=your_azure_api_key
MISTRAL_API_KEY=your_mistral_api_key
DEVIN_API_KEY=your_devin_api_key

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/orchestrator
REDIS_URL=redis://localhost:6379

# Server Configuration
PORT=3000
NODE_ENV=development

# Agent Configuration
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT_MS=300000

# MCP Server
MCP_SERVER_PORT=4000
MCP_SECRET_KEY=your_mcp_secret_key

# Monitoring
ENABLE_TELEMETRY=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Git Integration
ENABLE_GIT=false
GIT_REPO_PATH=./
GIT_AUTHOR_NAME=Multi-Agent Orchestrator
GIT_AUTHOR_EMAIL=orchestrator@example.com
GIT_AUTO_COMMIT=true
GIT_COMMIT_PREFIX=[AI-Task]
GIT_BRANCH=main

# Slack Integration
SLACK_WEBHOOK_URL=
SLACK_CHANNEL=#general
SLACK_USERNAME=Multi-Agent Orchestrator
SLACK_ICON_EMOJI=:robot_face:

# Microsoft Teams Integration
TEAMS_WEBHOOK_URL=