{
  "name": "multi-agent-orchestrator",
  "displayName": "Multi-Agent Orchestrator",
  "description": "VS Code extension for the Multi-Agent Orchestrator system",
  "version": "0.1.0",
  "publisher": "multi-agent-orchestrator",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Debuggers"
  ],
  "keywords": [
    "ai",
    "agents",
    "orchestrator",
    "automation",
    "task-management"
  ],
  "activationEvents": [
    "onCommand:multiAgentOrchestrator.connect",
    "onView:multiAgentOrchestrator.tasks",
    "onView:multiAgentOrchestrator.knowledge"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multiAgentOrchestrator.connect",
        "title": "Connect to Orchestrator",
        "category": "Multi-Agent Orchestrator"
      },
      {
        "command": "multiAgentOrchestrator.disconnect",
        "title": "Disconnect",
        "category": "Multi-Agent Orchestrator"
      },
      {
        "command": "multiAgentOrchestrator.createTask",
        "title": "Create Task",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(add)"
      },
      {
        "command": "multiAgentOrchestrator.executeTask",
        "title": "Execute Task",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(play)"
      },
      {
        "command": "multiAgentOrchestrator.refreshTasks",
        "title": "Refresh Tasks",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(refresh)"
      },
      {
        "command": "multiAgentOrchestrator.openWebDashboard",
        "title": "Open Web Dashboard",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(globe)"
      },
      {
        "command": "multiAgentOrchestrator.searchKnowledge",
        "title": "Search Knowledge Base",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(search)"
      },
      {
        "command": "multiAgentOrchestrator.createKnowledgeEntry",
        "title": "Create Knowledge Entry",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(note)"
      },
      {
        "command": "multiAgentOrchestrator.generateFromTemplate",
        "title": "Generate from Template",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(file-code)"
      },
      {
        "command": "multiAgentOrchestrator.runSecurityScan",
        "title": "Run Security Scan",
        "category": "Multi-Agent Orchestrator",
        "icon": "$(shield)"
      }
    ],
    "views": {
      "multiAgentOrchestrator": [
        {
          "id": "multiAgentOrchestrator.tasks",
          "name": "Tasks",
          "when": "multiAgentOrchestrator.connected"
        },
        {
          "id": "multiAgentOrchestrator.agents",
          "name": "Agents",
          "when": "multiAgentOrchestrator.connected"
        },
        {
          "id": "multiAgentOrchestrator.knowledge",
          "name": "Knowledge",
          "when": "multiAgentOrchestrator.connected"
        },
        {
          "id": "multiAgentOrchestrator.templates",
          "name": "Templates",
          "when": "multiAgentOrchestrator.connected"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "multiAgentOrchestrator",
          "title": "Multi-Agent Orchestrator",
          "icon": "$(robot)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "multiAgentOrchestrator.createTask",
          "when": "view == multiAgentOrchestrator.tasks",
          "group": "navigation"
        },
        {
          "command": "multiAgentOrchestrator.refreshTasks",
          "when": "view == multiAgentOrchestrator.tasks",
          "group": "navigation"
        },
        {
          "command": "multiAgentOrchestrator.searchKnowledge",
          "when": "view == multiAgentOrchestrator.knowledge",
          "group": "navigation"
        },
        {
          "command": "multiAgentOrchestrator.createKnowledgeEntry",
          "when": "view == multiAgentOrchestrator.knowledge",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "multiAgentOrchestrator.executeTask",
          "when": "view == multiAgentOrchestrator.tasks && viewItem == task",
          "group": "inline"
        }
      ],
      "editor/context": [
        {
          "command": "multiAgentOrchestrator.createTask",
          "when": "editorHasSelection",
          "group": "orchestrator"
        },
        {
          "command": "multiAgentOrchestrator.searchKnowledge",
          "when": "editorHasSelection",
          "group": "orchestrator"
        }
      ],
      "commandPalette": [
        {
          "command": "multiAgentOrchestrator.executeTask",
          "when": "false"
        }
      ]
    },
    "configuration": {
      "title": "Multi-Agent Orchestrator",
      "properties": {
        "multiAgentOrchestrator.serverUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "URL of the Multi-Agent Orchestrator server"
        },
        "multiAgentOrchestrator.apiKey": {
          "type": "string",
          "description": "API key for authentication (if required)"
        },
        "multiAgentOrchestrator.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Automatically refresh data from server"
        },
        "multiAgentOrchestrator.refreshInterval": {
          "type": "number",
          "default": 30,
          "description": "Refresh interval in seconds"
        },
        "multiAgentOrchestrator.enableNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show notifications for task status changes"
        },
        "multiAgentOrchestrator.defaultTaskType": {
          "type": "string",
          "enum": [
            "requirement",
            "design",
            "implementation",
            "test",
            "deployment",
            "review"
          ],
          "default": "implementation",
          "description": "Default task type for new tasks"
        },
        "multiAgentOrchestrator.defaultPriority": {
          "type": "string",
          "enum": [
            "low",
            "medium",
            "high",
            "critical"
          ],
          "default": "medium",
          "description": "Default priority for new tasks"
        }
      }
    },
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "python",
        "path": "./snippets/python.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "20.x",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "typescript": "^5.3.2",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "ws": "^8.14.2"
  }
}