    "body": [
      "// TODO: Multi-Agent Orchestrator Task",
      "// Title: ${1:Task Title}",
      "// Type: ${2|requirement,design,implementation,test,deployment,review|}",
      "// Priority: ${3|low,medium,high,critical|}",
      "// Description: ${4:Task description}",
      "// Requirements:",
      "// - ${5:Requirement 1}",
      "// - ${6:Requirement 2}",
      "",
      "${0:// Your code here}"
    ],
    "description": "Create a Multi-Agent Orchestrator task comment"
  },
  
  "MAO API Client": {
    "prefix": "mao-client",
    "body": [
      "import axios from 'axios';",
      "",
      "class OrchestratorClient {",
      "  private apiClient = axios.create({",
      "    baseURL: '${1:http://localhost:3000/api}',",
      "    headers: {",
      "      'Content-Type': 'application/json',",
      "      'Authorization': 'Bearer ${2:your-api-key}'",
      "    }",
      "  });",
      "",
      "  async createTask(task: {",
      "    title: string;",
      "    description: string;",
      "    type: 'requirement' | 'design' | 'implementation' | 'test' | 'deployment' | 'review';",
      "    priority: 'low' | 'medium' | 'high' | 'critical';",
      "  }) {",
      "    const response = await this.apiClient.post('/tasks', task);",
      "    return response.data;",
      "  }",
      "",
      "  async executeTask(taskId: string) {",
      "    const response = await this.apiClient.post(`/tasks/${taskId}/execute`);",
      "    return response.data;",
      "  }",
      "",
      "  async getTasks() {",
      "    const response = await this.apiClient.get('/tasks');",
      "    return response.data.tasks;",
      "  }",
      "}",
      "",
      "export const orchestrator = new OrchestratorClient();"
    ],
    "description": "Create an API client for Multi-Agent Orchestrator"
  },

