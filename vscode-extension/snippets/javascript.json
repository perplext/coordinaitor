  "MAO WebSocket Client": {
    "prefix": "mao-websocket",
    "body": [
      "const WebSocket = require('ws');",
      "",
      "class OrchestratorWebSocket {",
      "  constructor(url = '${1:ws://localhost:3000}') {",
      "    this.url = url;",
      "    this.ws = null;",
      "    this.reconnectInterval = ${2:5000};",
      "  }",
      "",
      "  connect() {",
      "    this.ws = new WebSocket(this.url);",
      "    ",
      "    this.ws.on('open', () => {",
      "      console.log('Connected to orchestrator');",
      "    });",
      "    ",
      "    this.ws.on('message', (data) => {",
      "      const message = JSON.parse(data.toString());",
      "      this.handleMessage(message);",
      "    });",
      "    ",
      "    this.ws.on('close', () => {",
      "      console.log('Disconnected from orchestrator');",
      "      setTimeout(() => this.connect(), this.reconnectInterval);",
      "    });",
      "    ",
      "    this.ws.on('error', (error) => {",
      "      console.error('WebSocket error:', error);",
      "    });",
      "  }",
      "",
      "  handleMessage(message) {",
      "    switch (message.type) {",
      "      case 'task:created':",
      "        console.log('Task created:', message.data.title);",
      "        break;",
      "      case 'task:completed':",
      "        console.log('Task completed:', message.data.title);",
      "        break;",
      "      case 'task:failed':",
      "        console.log('Task failed:', message.data.title);",
      "        break;",
      "      default:",
      "        console.log('Unknown message type:', message.type);",
      "    }",
      "  }",
      "",
      "  disconnect() {",
      "    if (this.ws) {",
      "      this.ws.close();",
      "    }",
      "  }",
      "}",
      "",
      "module.exports = { OrchestratorWebSocket };"
    ],
    "description": "Create a WebSocket client for real-time orchestrator updates"
  }
}