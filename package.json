{
  "name": "coordinaitor",
  "version": "0.1.0",
  "description": "Ultimate AI coordination system for orchestrating coding agents",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:all": "npm run build && cd web && npm run build",
    "dev": "tsx watch src/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"cd web && npm run dev\"",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/services tests/orchestration",
    "test:integration": "jest tests/integration",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test:smoke": "jest tests/smoke",
    "test:health": "jest tests/health",
    "test:e2e": "cd web && npm run test:e2e",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "db:seed": "tsx src/database/seed.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "claude",
    "gemini",
    "codex"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@aws-sdk/client-bedrock-runtime": "^3.540.0",
    "@aws-sdk/client-qbusiness": "^3.540.0",
    "@azure/openai": "^1.0.0-beta.12",
    "@google/generative-ai": "^0.15.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.5",
    "openai": "^4.28.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.2.2",
    "samlify": "^2.8.10",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.25",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
