{
  "name": "CoordinAItorMobile",
  "version": "1.0.0",
  "description": "Mobile app for CoordinAItor platform",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
